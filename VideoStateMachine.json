{
    "Comment": "The typical video workflow step function",
    "StartAt": "Split",
    "States": {
      "Split": {
        "Type": "Task",
        "Resource": "arn:aws:lambda:us-east-2:693949087897:function:ahlFramer",
        "Next": "recNremove"
      },
      "recNremove": {
        "Type": "Parallel",
        "Next": "BatchPut",
        "Branches": [
          {
            "StartAt": "Iterate",
            "States": {
              "Iterate": {
                "Type": "Map",
                "InputPath": "$.detail",
                "ItemsPath": "$.items",
                "Iterator": {
                  "StartAt": "Recognize",
                  "States": {
                    "Recognize": {
                      "Type": "Task",
                      "Resource": "arn:aws:lambda:us-east-2:693949087897:function:ahlFrameIdentifier",
                      "Next": "Put"
                    },
                    "Put" :{
                      "Type": "Pass",
                      "Result": " Metto su DynamoDB",
                      "End": true
                    }
                  }
                },
                "End": true
              }
            }
          },
          {
            "StartAt": "RemoveLow",
            "States": {
              "RemoveLow": {
                "Type": "Task",
                "Resource": "arn:aws:lambda:us-east-2:693949087897:function:lowRemove",
                "End": true
              }
            }
          }
        ]
      },
      "BatchPut": {
        "Type": "Pass",
        "Result": "All the frame infos are in DynamoDB",
        "Next": "CreateResume"
      },
      "CreateResume": {
        "Type": "Pass",
        "Result": "Fetches info from dynamo and preceed creating the resume file",
        "End": true
      }
    }
  }