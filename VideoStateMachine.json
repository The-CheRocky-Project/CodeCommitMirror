{
    "Comment": "The typical video workflow",
    "StartAt": "prepareData",
    "States": {
      "prepareData": {
        "Type": "Task",
        "Resource": "arn:aws:lambda:us-east-2:693949087897:function:startMachine",
        "Next": "removeLow"
      },
      "removeLow": {
        "Type": "Task",
        "Resource": "arn:aws:lambda:us-east-2:693949087897:function:lowRemove",
        "Next": "Check"
      },
      "Check": {
        "Type": "Choice",
        "Choices":[
            {
               "Variable":"$.continue",
               "StringEquals":"false",
               "Next":"iteratorMaster"
            }
         ],
         "Default":"CreateResume"
      },
      "iteratorMaster": {
        "Type": "Task",
        "Resource": "arn:aws:lambda:us-east-2:693949087897:function:prepareForReconize",
        "Next": "Iterate"
      },
      "Iterate": {
        "Type": "Map",
        "InputPath": "$.detail",
        "ItemsPath": "$.items",
        "Iterator": {
          "StartAt": "Recognize",
          "States": {
            "Recognize": {
              "Type": "Task",
              "Resource": "arn:aws:lambda:us-east-2:693949087897:function:ahlFrameIdentifier",
              "End": true
            }
          }
        },
        "ResultPath": "$.detail.items",
        "Next":"Check"
      },
      "CreateResume": {
        "Type": "Pass",
        "Result": "Fetches info from dynamo and preceed creating the resume file",
        "End": true
      }
    }
  }